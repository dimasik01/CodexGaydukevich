<#macro page>
<!DOCTYPE HTML>
<html>
<head>
    <title>Gaydukevich</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>

<style type="text/css">
        body{
            background-color: #FFFFFF;
        }
        h1 {
          color: black;
          font-size: 40px;
          font-weight: bold;
        }
        h2 {
          color: white;
        }
        h3 {
          color: black;
          font-size: 24px;
          font-weight: bold;
        }
        label {
          color: black;
          font-size: 24px;
          font-weight: bold;
        }
        table {
            background-color: #ffffff;
            -moz-border-radius:8px;
            -webkit-border-radius:8px;
            border-radius:8px;
            padding: 8px;
        }
        input[type=submit] {
          background-color: #fffff;
          color: black;
          border-radius: 16px;
          cursor: pointer;
          padding: 16px;
          font-size: 16px;
          font-weight: bold;
        }
        button[type=submit] {
          background-color: #fffff;
          color: black;
          border-radius: 16px;
          cursor: pointer;
          padding: 16px;
          font-size: 16px;
          font-weight: bold;
        }
        input[type=text] {
          background-color: #ffffff;
          color: black;
          border-radius: 16px;
          padding: 16px;
          font-size: 16px;
          font-weight: bold;
        }
        input[type=password] {
           background-color: #ffffff;
           color: black;
           border-radius: 16px;
           padding: 16px;
           font-size: 16px;
           font-weight: bold;
        }
        input[type=submit]:hover {
          background-color: #d1d1d1;
        }
        button[type=submit]:hover {
          background-color: #d1d1d1;
        }
        th, td {
          border: 1px solid black;
          padding: 6px;
          font-weight: bold;
        }
        .center{
                    position: absolute;
                    top: 50%;
                    left: 50%;
                    margin: -100px 0 0 -100px;
                }
                .center1{
                                    position: absolute;
                                    top: 20%;
                                    left: 40%;
                                }
                                .center2{
                                                                    position: absolute;
                                                                    top: 20%;
                                                                    left: 40%;
                                                                }
    </style>
</head>

<div align="center">

<#nested>

</div>

</body>
</html>
